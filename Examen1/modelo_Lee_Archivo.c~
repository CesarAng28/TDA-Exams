#include"TDA_Statistics.h"

float * modelo_Lee_Archivo(FILE * Archivo)
{
  int count_datos = 0;
  
  int temp;
  float temp2;
  
  float * data_array;

  //Primero lo leemos sin guardar nada para determinar su tama√±o.
  while(!feof(Archivo))
  {
    fscanf(Archivo,"%d,%f\n",&temp,&temp2);
    count_datos++;
  }

  printf("\n\n%d datos\n",count_datos);
}
